{
    "manifest_version":2,

    "name": "Sloth",
    "description": "This extension will help you calm down",
    "version":"1.0",
    "content_scripts": [
    {
        "matches":["<all_urls>"],
        "js": ["jquery-1.4.2.js", "content.js"]
    }
    ],

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "sloth.png",
        "default_popup":"popup.html"
    }
}